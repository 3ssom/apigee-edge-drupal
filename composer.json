{
    "name": "drupal/apigee_edge",
    "license": "MIT",
    "type": "drupal-module",
    "description": "Apigee Edge Drupal integration.",
    "require": {
        "php": ">=7.1",
        "php-http/guzzle6-adapter": "^1.1.1",
        "apigee/edge": "dev-2.x-dev",
        "drupal/core": "^8.5",
        "drupal/entity": "^1.0",
        "drupal/key": "^1.7"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "phpunit/phpunit": "^6.1"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^9"
            }
        }
    }
}
