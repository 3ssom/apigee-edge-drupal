{
    "name": "drupal/apigee_edge",
    "license": "MIT",
    "type": "drupal-module",
    "description": "Apigee Edge Drupal integration.",
    "require": {
        "php": ">=7.1",
        "php-http/guzzle6-adapter": "^1.1.1",
        "apigee/edge": "dev-2.x-dev",
        "drupal/core": "^8.5",
        "drupal/entity": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^9"
            }
        }
    }
}
